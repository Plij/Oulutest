particle_system Rocket_smoke.particle
{
    // Main attributes
    quota 60
    material smoke.material
    particle_width 0.55
    particle_height 0.55
    cull_each true

    // Billboard renderer attributes
    billboard_rotation_type vertex

    // Emitters
    emitter Point
    {
        direction 0 -1 0
        up -0 0 1
        emission_rate 100
        position 0 -2 0
        velocity 0.5
        velocity_min 0.5
        velocity_max 0.9
        time_to_live 2
        time_to_live_min 2
        time_to_live_max 6
    }

    emitter Point
    {
        direction 0 0 0
        up 0 0 0
        position 0 -7 0
        velocity 1.5
        velocity_min 1.5
        velocity_max 10.9
        time_to_live 2
        time_to_live_min 2
        time_to_live_max 16
    }

    // Affectors
    affector ColourFader
    {
        red -0.1
        green -0.1
        blue -0.1
        alpha -0.5
    }

    affector Scaler
    {
        rate 0.5
    }

    affector ColourInterpolator
    {
        colour0 0 0 128 1
        time0 0
        colour1 0 0 128 0.490196
        time1 0.2
        colour2 0 0 128 1
        time2 0.4
        colour3 0 0 128 0.698039
        time3 0.6
        colour4 0 0 128 0.498039
        time4 0.8
        colour5 0 0 128 1
        time5 1
    }

    affector Rotator
    {
        rotation_speed_range_start -100
        rotation_speed_range_end 100
        rotation_range_end 100
    }
}
